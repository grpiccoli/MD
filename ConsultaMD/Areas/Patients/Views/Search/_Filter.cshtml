@model MapVM

    <div class="row btn-controls">
        <div class="left">
            <button class="btn filter-action btn-small waves-effect waves-light">
                <i class="material-icons left">arrow_back</i>
                Aceptar
            </button>
        </div>
        <div id="filter-options" class="fixed-action-btn toolbar direction-top">
            <a href="#" class="btn-floating btn-small red">
                <i class="small material-icons">more</i>
                <div class="fab-backdrop"></div>
            </a>
            <ul>
                <li class="waves-effect waves-light red">
                    <a href="#!" id="filter-clear">
                        <i class="material-icons left">do_not_disturb</i>
                        Eliminar filtros
                    </a>
                </li>
                <li class="waves-effect waves-light teal">
                    <a href="#!" id="filter-clear">
                        <i class="material-icons right">close</i>
                        Cerrar
                    </a>
                </li>
            </ul>
        </div>
        <div class="right">
            <button class="btn filter-action btn-small waves-effect red lighten-2 waves-light">
                <i class="material-icons right">close</i>
                Cancelar
            </button>
        </div>
    </div>
