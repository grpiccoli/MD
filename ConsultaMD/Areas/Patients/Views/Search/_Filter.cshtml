@model MapVM

<div class="dates-div"></div>
<div class="especialities-div"></div>
<div class="locations-div"></div>
<div class="col s12 m6">
    <div class="card">
        <div class="card-content clinic-text">
            <span class="card-title">
                Sexo del Profesional Médico
            </span>
        </div>
        <div class="card-action">
            <div class="row">
                <div class="col s12">
                    <select asp-for="Sex">
                        <option></option>
                        <option value="false">Femenino</option>
                        <option value="true">Masculino</option>
                    </select>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="col s12 m6">
    <div class="card">
        <div class="card-content clinic-text">
            <span class="card-title">
                Convenios
            </span>
        </div>
        <div class="card-action">
            <div class="row">
                <div class="col s12">
                    <input @(Model.Insurance == ConsultaMD.Data.InsuranceData.Insurance.Particular ? "disabled" : "") asp-for="HighlightInsurance" class="filled-in"/>
                    <span>
                        Destacar servicios compatibles con plan de salud
                    </span>
                </div>
            </div>
        </div>
    </div>
</div>