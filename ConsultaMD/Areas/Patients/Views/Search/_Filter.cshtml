        <div id="especialities-col" class="col s12 fltcol">
            <div class="card">
                <div class="card-content clinic-text">
                    <span class="card-title">
                        Especialidades
                    </span>
                </div>
                <div class="card-action">
                    <div class="row">
                        <div class="col s12">
                            <select asp-for="Especialidad" multiple="multiple" class="sel2open"></select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="locations-col" class="col s12 fltcol">
            <div class="card">
                <div class="card-content clinic-text">
                    <span class="card-title">
                        Comuna, Provincia o Región
                    </span>
                </div>
                <div class="card-action">
                    <div class="row">
                        <div class="col s12">
                            <select asp-for="Ubicacion" multiple="multiple" class="sel2open"></select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col s12 fltcol">
            <div class="card">
                <div class="card-content clinic-text">
                    <span class="card-title">
                        Género del Profesional Médico
                    </span>
                </div>
                <div class="card-action">
                    <div class="row">
                        <div class="col s12">
                            <select asp-for="Sex" class="sel2open">
                                <option value="false">♀ Femenino</option>
                                <option value="true">♂ Masculino</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col s12 fltcol">
            <div class="card">
                <div class="card-content clinic-text">
                    <span class="card-title">
                        Convenios
                    </span>
                </div>
                <div class="card-action">
                    <div class="row">
                        <div class="col s12">
                            <p>
                                <label>
                                    <input asp-for="HighlightInsurance" disabled="@(Model.Insurance == 0)" class="validate filled-in" />
                                    <span>@Html.DisplayNameFor(model => model.HighlightInsurance)</span>
                                </label>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
