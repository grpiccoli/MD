@model List<ReservationDetails>

<div class="row">
    @foreach (var res in Model)
    {
    <div class="col s12 m6">
        <div class="card">
            <div class="card-content">
                <span class="card-title">Reserva</span>
            </div>
            <div class="card-tabs">
                <ul class="tabs tabs-fixed-width">
                    <li class="tab"><a href="#timeslot">Fecha</a></li>
                    <li class="tab"><a href="#place">Lugar</a></li>
                    <li class="tab"><a href="#doctor">Doctor</a></li>
                    <li class="tab"><a href="#patient">Paciente</a></li>
                </ul>
            </div>
            <div class="card-content">
                <div id="timeslot">
                    @res.TimeSlotDetails.DateTime
                </div>
                <div id="place">
                    <span>@res.PlaceDetails.Name</span>
                    <span>@res.PlaceDetails.Address</span>
                </div>
                <div id="doctor">
                    <span>@res.DoctorDetails.TitleName</span>
                    <span>@res.DoctorDetails.Rut</span>
                    <span>@res.DoctorDetails.SpecialtyList</span>
                </div>
                <div id="patient">
                    <span>@res.PatientDetails.Name</span>
                    <span>@res.PatientDetails.Rut</span>
                </div>
            </div>
            <div class="card-action">
                <a href="#">Pagar</a>
                <a href="#">Eliminar</a>
            </div>
        </div>
    </div>
    }
</div>